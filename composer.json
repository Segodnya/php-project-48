{
  "name": "hexlet/code",
  "description": "Hexlet Diff Generator Project",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "files": [
      "src/fileParser.php",
      "src/Differ.php"
    ],
    "psr-4": {
      "Differ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Differ\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "segodnya",
      "email": "immortalsteel@gmail.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.2"
    }
  },
  "bin": [
    "bin/gendiff"
  ],
  "scripts": {
    "test": "phpunit tests"
  },
  "require": {
    "docopt/docopt": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "phpunit/php-code-coverage": "^9.0" ,
    "squizlabs/php_codesniffer": "3.10",
    "phpstan/phpstan": "^1.12"
  }
}
